{
	"version": "0.1.0",
	"license": "MIT",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"bin": {
		"tsdoc-xml": "./bin/tsdoc-xml.js"
	},
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "tsdx watch",
		"build": "tsdx build",
		"test": "tsdx test",
		"lint": "tsdx lint",
		"prepare": "tsdx build",
		"size": "size-limit",
		"analyze": "size-limit --why"
	},
	"peerDependencies": {},
	"husky": {
		"hooks": {
			"pre-commit": "tsdx lint"
		}
	},
	"prettier": {
		"printWidth": 100,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "none",
		"useTabs": true
	},
	"name": "tsdoc-xml",
	"author": "wvbe",
	"module": "dist/tsdoc-xml.esm.js",
	"size-limit": [
		{
			"path": "dist/tsdoc-xml.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/tsdoc-xml.esm.js",
			"limit": "10 KB"
		}
	],
	"devDependencies": {
		"@size-limit/preset-small-lib": "^4.10.2",
		"husky": "^6.0.0",
		"size-limit": "^4.10.2",
		"tsdx": "^0.14.1",
		"tslib": "^2.2.0",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"@microsoft/tsdoc": "^0.13.0"
	}
}